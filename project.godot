config_version=5

[application]

config/name="DigitalTwinVisualisationProject"
run/main_scene="res://2DVisualizationScene/2DVisualisationScene.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

LogDuck="*res://addons/LogDuck/LogDuck.gd"
StyleConfig="*res://Config/StyleConfig.gd"
FusekiConfig="*res://Config/FusekiConfig.gd"
FusekiSignals="*res://Fuseki/Data/FusekiSignals.gd"
